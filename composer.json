{
    "name": "sandwich/symfony-lock-stash",
    "description": "Stash storage implementation for the Symfony Lock component",
    "type": "library",
    "require": {
        "php": "^7.2",
        "symfony/lock": "^3.4 || ^4.4",
        "tedivm/stash": "^0.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "myonlinestore/coding-standard": "^2.0",
        "vimeo/psalm": "^3.11"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sandwich BV",
            "email": "info@myonlinestore.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Sandwich\\Symfony\\Lock\\Store\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sandwich\\Tests\\Symfony\\Lock\\Store\\": "tests"
        }
    }
}
